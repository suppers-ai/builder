{
  "name": "@suppers/shared",
  "version": "1.0.0",
  "exports": {
    ".": "./mod.ts",
    "./types": "./types/mod.ts",
    "./utils": "./utils/mod.ts",
    "./utils/component-route-generator.tsx": "./utils/component-route-generator.tsx",
    "./utils/preview-generator.ts": "./utils/preview-generator.ts",
    "./utils/type-mappers.ts": "./utils/type-mappers.ts",
    "./constants": "./constants/mod.ts",
    "./schemas": "./schemas/mod.ts",
    "./generated/database-types": "./generated/database-types.ts"
  },
  "imports": {
    "@std/": "https://deno.land/std@0.224.0/",
    "zod": "https://esm.sh/zod@3.25.1",
    "@supabase/supabase-js": "https://esm.sh/@supabase/supabase-js@2.50.3",
    "preact/jsx-runtime": "npm:preact@10.26.9/jsx-runtime",
    "preact/jsx-dev-runtime": "npm:preact@10.26.9/jsx-dev-runtime"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.asynciterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "strict": true
  },
  "fmt": {
    "options": { "indentWidth": 2, "lineWidth": 100, "semiColons": true }
  },
  "lint": {
    "rules": { "tags": ["recommended"] }
  }
}
