  ],
  "routes": [
    {
      "path": "/",
      "component": "homePage",
      "layout": "mainLayout",
      "middleware": ["logRequest"],
      "meta": {
        "title": "Home | E-Commerce Store",
        "description": "Welcome to our online store featuring quality products at great prices"
      }
    },
    {
      "path": "/products",
      "component": "productListingPage",
      "layout": "mainLayout",
      "middleware": ["logRequest"],
      "meta": {
        "title": "Products | E-Commerce Store",
        "description": "Browse our complete catalog of products"
      }
    },
    {
      "path": "/products/:id",
      "component": "productDetailPage",
      "layout": "mainLayout",
      "middleware": ["logRequest"],
      "meta": {
        "title": "Product Details | E-Commerce Store",
        "description": "View detailed product information"
      }
    },
    {
      "path": "/cart",
      "component": "shoppingCartPage",
      "layout": "mainLayout",
      "middleware": ["logRequest"],
      "meta": {
        "title": "Shopping Cart | E-Commerce Store",
        "description": "Review items in your shopping cart"
      }
    },
    {
      "path": "/checkout",
      "component": "checkoutPage",
      "layout": "mainLayout",
      "middleware": ["logRequest", "requireAuth"],
      "meta": {
        "title": "Checkout | E-Commerce Store",
        "description": "Complete your purchase",
        "requiresAuth": true
      }
    }
  ],
  "api": {
    "endpoints": [
      {
        "path": "/api/products",
        "methods": ["GET"],
        "handler": "ProductsHandler",
        "middleware": ["validateRequest"]
      },
      {
        "path": "/api/products/:id",
        "methods": ["GET"],
        "handler": "ProductDetailHandler",
        "middleware": ["validateRequest"],
        "validation": {
          "params": {
            "id": {
              "type": "string",
              "required": true,
              "pattern": "^[0-9a-fA-F]{24}$"
            }
          }
        }
      },
      {
        "path": "/api/cart",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "handler": "CartHandler",
        "middleware": ["validateRequest", "requireAuth"],
        "validation": {
          "body": {
            "productId": {
              "type": "string",
              "required": true,
              "pattern": "^[0-9a-fA-F]{24}$"
            },
            "quantity": {
              "type": "number",
              "required": true,
              "min": 1,
              "max": 100
            }
          }
        },
        "auth": {
          "required": true
        }
      },
      {
        "path": "/api/orders",
        "methods": ["GET", "POST"],
        "handler": "OrdersHandler",
        "middleware": ["validateRequest", "requireAuth"],
        "validation": {
          "body": {
            "shippingAddress": {
              "type": "object",
              "required": true,
              "properties": {
                "name": {
                  "type": "string",
                  "required": true
                },
                "address": {
                  "type": "string",
                  "required": true
                },
                "city": {
                  "type": "string",
                  "required": true
                },
                "zipCode": {
                  "type": "string",
                  "required": true
                }
              }
            },
            "paymentMethod": {
              "type": "string",
              "required": true,
              "enum": ["credit_card", "paypal"]
            }
          }
        },
        "auth": {
          "required": true
        }
      }
    ],
    "middleware": [
      {
        "name": "logRequest",
        "config": {
          "level": "info"
        },
        "order": 0
      },
      {
        "name": "validateRequest",
        "order": 1
      },
      {
        "name": "requireAuth",
        "config": {
          "redirectTo": "/login"
        },
        "order": 2
      }
    ],
    "auth": {
      "provider": "jwt",
      "config": {
        "secret": "your-secret-key",
        "expiresIn": "1d"
      },
      "routes": {
        "login": "/api/auth/login",
        "logout": "/api/auth/logout",
        "register": "/api/auth/register"
      }
    }
  },
  "theme": {
    "primaryColor": "#3b82f6",
    "secondaryColor": "#64748b",
    "accentColor": "#f59e0b",
    "fontFamily": "Inter, system-ui, sans-serif",
    "spacing": {
      "xs": "0.5rem",
      "sm": "1rem",
      "md": "1.5rem",
      "lg": "2rem",
      "xl": "3rem"
    },
    "breakpoints": {
      "sm": "640px",
      "md": "768px",
      "lg": "1024px",
      "xl": "1280px",
      "2xl": "1536px"
    }
  },
  "build": {
    "target": "production",
    "minify": true,
    "sourceMaps": false,
    "outputDir": "./dist",
    "publicPath": "/"
  }
}